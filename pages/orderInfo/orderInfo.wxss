page {
  background-color: #f4f4f4;
}
.fixBtnGroup {
	position: fixed; bottom: 0; width: 100%; height: 130rpx;
	box-shadow:0 2px 6px 0 rgba(251,147,41,0.70);
	text-align: center;
}
.fixBtnGroup .fixBtn {
	width: 50%;
	height: 100%;
	float: left;
	font-size:16px;
	padding-top: 20rpx;
}
.fixBtn.left {
	background:#ffffff;
	color:#646464;
}
.fixBtn.right {
	background:#fa9b18;
	color: #fff;
}